{
  "name": "ng-formalizer",
  "version": "0.0.0",
  "description": "Angular (&UI) form generator and validation from JSON",
  "main": "./index.js",
  "keywords": [],
  "directories": {
    "lib": ".",
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/karma/bin/karma start karma.js --single-run --angular=1.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/inetsys/ng-formalizer.git"
  },
  "author": {
    "name": "Luis Lafuente",
    "email": "llafuente@noboxout.com",
    "url": "http://www.noboxout.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.10"
  },
  "dependencies": {},
  "devDependencies": {
    "body-parser": "^1.7.0",
    "bootstrap-grunt": "0.0.2",
    "express": "*",
    "grunt": "^0.4.5",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-cssmin": "^0.10.0",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-html2js": "^0.2.8",
    "grunt-release": "^0.7.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-ng-html2js-preprocessor": "^0.1.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "jasmine-core": "*",
    "phantomjs": "*",
    "phantomjs": "^1.9.18",
    "serve-static": "*"
  },
  "jshintConfig": {
    "white": true,
    "curly": true,
    "eqeqeq": true,
    "asi": false,
    "boss": false,
    "browser": true,
    "devel": true,
    "maxdepth": 4,
    "maxstatements": 25,
    "maxcomplexity": 6,
    "eqnull": true,
    "es5": true,
    "evil": false,
    "immed": false,
    "jquery": true,
    "latedef": false,
    "laxcomma": true,
    "laxbreak": true,
    "newcap": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "predef": [
      "log",
      "angular",
      "describe",
      "beforeEach",
      "inject",
      "it",
      "expect"
    ],
    "quotmark": "single",
    "smarttabs": false,
    "strict": false,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}
