{
  "name": "ng-formalizer",
  "version": "0.0.2",
  "description": "Angular (&UI) form generator and validation from JSON",
  "main": "./index.js",
  "keywords": [],
  "directories": {
    "lib": ".",
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/karma/bin/karma start karma.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/llafuente/ng-formalizer.git"
  },
  "author": {
    "name": "Luis Lafuente",
    "email": "llafuente@noboxout.com",
    "url": "http://www.noboxout.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.10"
  },
  "dependencies": {},
  "devDependencies": {
    "express": "*",
    "serve-static": "*",
    "karma": "^0.12.19",
    "karma-ng-html2js-preprocessor": "^0.1.0",
    "karma-jasmine": "^0.1.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-coverage": "^0.2.5",
    "karma-firefox-launcher": "^0.1.3",
    "body-parser": "^1.7.0",
    "grunt": "^0.4.5",
    "grunt-html2js": "^0.2.8",
    "bootstrap-grunt": "0.0.2",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-release": "^0.7.0"
  },
  "jshintConfig": {
    "white": true,
    "curly": true,
    "eqeqeq": true,
    "asi": false,
    "boss": false,
    "browser": true,
    "devel": true,
    "maxdepth": 4,
    "maxstatements": 25,
    "maxcomplexity": 6,
    "eqnull": true,
    "es5": true,
    "evil": false,
    "immed": false,
    "jquery": true,
    "latedef": false,
    "laxcomma": true,
    "laxbreak": true,
    "newcap": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "predef": [
      "log"
    ],
    "quotmark": "double",
    "smarttabs": false,
    "strict": false,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}
