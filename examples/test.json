{
    "legend": "json-legend",
    "type": "horizontal",
    "name": "form002",
    "onSubmit": "SendToServer",
    "model": "entity2",
    "fields": [{
        "label": "Text field",
        "type": "text",
        "name": "user",
        "placeholder": "Text",
        "constraints": {
            "required": true
        }
    },{
        "label": "Password field",
        "type": "password",
        "name": "pwd",
        "placeholder": "Password",
        "constraints": {
            "only-iso": true
        }
    },{
        "label": "Password field - 2",
        "type": "password",
        "name": "pwd2",
        "placeholder": "repeat password",
        "constraints": {
            "equal-to": "entity2.pwd"
        },
        "messages": {
            "equal-to": "La contrase√±a no coincide."
        }
    },{
        "label": "T&C",
        "type": "checkbox",
        "name": "tyc",
        "right": true,
        "constraints": {
            "required": "entity2.user"
        }
    },{
        "label": "Services",
        "type": "checkbox-list",
        "name": "services",
        "source": "src_list"
    },{
        "label": "Datepicker",
        "type": "datepicker",
        "name": "date"
    },{
        "label": "Platform",
        "type": "select",
        "name": "platform",
        "source": [{"id": "mobile", "label": "mobile"},{"id": "desktop", "label": "desktop"}],
        "empty_msg": "Please choose a car"
    },{
        "type": "select-bool",
        "label": "IPhone",
        "option_id": "id",
        "option_value": "value",
        "source": [{"id": true, "label": "YES"},{"id": false, "label": "NO"}],
        "name": "has_iphone"
    },{
        "label": "Lista",
        "type": "select",
        "name": "lista",
        "source": "list",
        "empty_msg": "Choose one"
    },{
        "label": "Submit now!",
        "type": "submit",
        "name": "submit"
    }]
}